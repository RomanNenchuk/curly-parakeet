  "questions": {
    "username_change": {
      "title": "Can I change my username?",
      "answer": "Yes, you can always change your username on the profile update page. However, it is important to note that you cannot choose a username that has already been used by another user."
    },
    "forgot_password": {
      "title": "How can I log in if I forgot my password?",
      "answer": "Click 'Forgot password?' on the login page and follow the further instructions."
    },
    "search_system": {
      "title": "How does the search system work?",
      "answer": {
        "general": "The search system allows you to find UFORUM users and tags used for creating topics, or a combination of both.",
        "first": "1. The ‘@’ symbol is used to search for users in the application.",
        "second": "2. The ‘#’ symbol is used to search for tags.",
        "third": "3. A combination of these symbols will suggest topics created by the specified user with the specified tag. Users and tags entered should be separated by punctuation marks: comma, slash, semicolon."
      }
    }
  }
